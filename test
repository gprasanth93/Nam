class CustomMemoryStore extends rateLimit.MemoryStore {
  constructor() {
    super();
  }

  increment(key, cb) {
    super.increment(key, (err, { total, resetTime }) => {
      if (err) return cb(err);

      // Additional logic for exceeded URLs
      if (total >= 10) {
        exceededUrls[key] = { total, resetTime };
      } else {
        delete exceededUrls[key];
      }

      cb(null, { total, resetTime });
    });
  }
}
